{
  "name": "tscli",
  "version": "0.1.2-alpha.3",
  "description": "TypeScript CLI project generator",
  "main": "./src/main.ts",
  "bin": {
    "cli": "bin/main.js"
  },
  "scripts": {
    "build": "tsc",
    "check": "tslint -t codeFrame ./src/*.ts",
    "clean": "shx rm -rf ./bin && shx rm -rf ./build",
    "dev": "tsc --watch",
    "doc": "typedoc --module commonjs --target ES5 --ignoreCompilerErrors --exclude node_modules --out ./docs/typedoc/ ./src",
    "format": "tsfmt -r --baseDir ./",
    "link": "npm link",
    "node:debug": "node --inspect --debug-brk ./bin/main.js",
    "postbuild": "run-s link",
    "prebuild": "run-s format check clean",
    "precommit": "run-s format check",
    "predoc": "shx rm -rf ./docs/typedoc && shx mkdir -p ./docs/typedoc",
    "prepush": "run-s test",
    "pretest": "run-s build",
    "test": "tape ./bin/lib/test/test.utils.js|tap-summary"
  },
  "keywords": [
    "cli",
    "javascript",
    "starter",
    "ts",
    "typescript"
  ],
  "author": "Rajinder Yadav <devguy.ca@gmail.com> (https://github.com/rajinder-yadav)",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rajinder-yadav/tscli"
  },
  "devDependencies": {
    "@types/commander": "^2.3.31",
    "@types/node": "^7.0.5",
    "@types/tape": "^4.2.29",
    "browser-sync": "^2.18.8",
    "browserify": "^14.1.0",
    "husky": "^0.13.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "shx": "^0.2.2",
    "tachyons": "^4.6.2",
    "tap-summary": "^3.0.1",
    "tape": "^4.6.3",
    "tape-run": "^2.2.1",
    "tslint": "^4.5.0",
    "typedoc": "^0.5.7",
    "typescript": "^2.2.1",
    "typescript-formatter": "^5.0.0"
  },
  "dependencies": {
    "bunyan": "^1.8.5",
    "commander": "^2.9.0",
    "shelljs": "^0.7.6"
  }
}
