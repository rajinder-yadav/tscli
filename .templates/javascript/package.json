{
  "name": "@tscli/demo",
  "version": "0.1.0",
  "description": "TypeScript CLI generated demo project",
  "main": "src/main.ts",
  "config": {
    "main": "src/main.js",
    "doc_folder": "docs/jsdoc"
  },
  "scripts": {
    "ava": "ava --tap \"src/**/*/test.*.js\"|tap-summary",
    "ava:coverage": "nyc ava --tap \"src/**/*/test.*.js\"|tap-summary",
    "check": "eslint \"src/**/*.js\"",
    "devwatch": "gazeall --run \"node src/main.js\" \"src/**/*\"",
    "doc": "jsdocs src $npm_package_config_doc_folder",
    "format": "prettier --write \"src/**/*.js\"",
    "node:debug": "node --inspect --debug-brk $npm_package_config_main",
    "precommit": "run-s format check",
    "predoc": "shx rm -rf $npm_package_config_doc_folder && shx mkdir -p $npm_package_config_doc_folder",
    "prepush": "npm run test",
    "start": "node $npm_package_config_main",
    "test": "npm run ava:coverage",
    "testwatch": "gazeall --runs-npm ava \"src/**/*\""

  },
  "keywords": [
    "javascript",
    "js",
    "nodejs"
  ],
  "author": "[Full name] <email-inside-brackets> (project-github-repo-inside bracker)",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/<github-user-id>/<project-name>"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "core-js": "^2.4.1",
    "cross-env": "^3.2.4",
    "cross-var": "^1.0.3",
    "eslint": "^3.17.1",
    "gazeall": "^0.2.5",
    "husky": "^0.13.2",
    "jsdocs": "^0.1.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.1.2",
    "prettier": "^0.22.0",
    "shx": "^0.2.2",
    "tachyons": "^4.6.2",
    "tap-summary": "^3.0.1"
  },
  "dependencies": {
    "bunyan": "^1.8.8"
  },
  "nyc": {
    "lines": 10,
    "statements": 10,
    "functions": 10,
    "branches": 10,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/**/test.*.js",
      "src/**/main.js"
    ],
    "reporter": [
      "text",
      "lcov",
      "text-summary",
      "html"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true
  }
}
